INCLUDE Irvine32.inc
;Author: John Slaughter
;Date: 11/25/2024
;Desc: Ch7 SE3
;shift instructions
.386
.model flat,stdcall
.stack 4096

.data
val1 SDWORD ?
;val2 SWORD -5
;val3 SWORD 3
mesV BYTE "val1 = ",0

ExitProcess proto, dwExitCode:dword
.code
main PROC


;write assembly code here
mov eax, 3	;eax = 00000003
mov ebx, -5	;ebx = FFFFFFFB
imul ebx	;eax = FFFFFFF1
mov val1, eax

;display to console
mov edx, offset mesV
call WriteString
mov edx, val1
call WriteInt
;display hex
call Crlf
mov edx, offset mesV
call WriteString
mov eax, val1
call WriteHex

INVOKE ExitProcess,0
main ENDP
END main
