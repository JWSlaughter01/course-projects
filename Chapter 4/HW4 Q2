.386
.model flat,stdcall
.stack 4096

.data
one SBYTE -100
two SBYTE -100

ExitProcess proto, dwExitCode:dword
.code
main PROC

;write assembly code here
mov eax, 0
mov al, one
add al, two
;mov bl, one ;throwawa





INVOKE ExitProcess,0
main ENDP
END main
