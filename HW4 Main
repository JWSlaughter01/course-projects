;Date: 10/10/2024
;Desc: COCS 2325 Homework 4 Main Exercise
;Carry Flag exercise
.386
.model flat,stdcall
.stack 4096

.data
array BYTE 02h, 05h, 07h, 08h, 01h, 03h
count EQU LENGTHOF array ;6


ExitProcess proto, dwExitCode:dword
.code
main PROC

;write assembly code here
mov ecx, 3
mov ebx, 0
mov esi, OFFSET array

L1:
mov al, [array + ebx]
mov ah, [array + ebx +1]
mov [array + ebx], ah
inc ebx
mov [array + ebx], al
inc ebx
loop L1

;mov eax, 0 ;throwaway command to help with viewing memory at end





INVOKE ExitProcess,0
main ENDP
END main
